	number = {7813},
	journaltitle = {Nature},
	author = {Kamitaki, Nolan and Sekar, Aswin and Handsaker, Robert E and de Rivera, Heather and Tooley, Katherine and Morris, David L and Taylor, Kimberly E and Whelan, Christopher W and Tombleson, Philip and Loohuis, Loes M Olde and Consortium, Schizophrenia Working Group of the Psychiatric Genomics and Boehnke, Michael and Kimberly, Robert P and Kaufman, Kenneth M and Harley, John B and Langefeld, Carl D and Seidman, Christine E and Pato, Michele T and Pato, Carlos N and Ophoff, Roel A and Graham, Robert R and Criswell, Lindsey A and Vyse, Timothy J and {McCarroll}, Steven A},
	urldate = {2020-08-27},
