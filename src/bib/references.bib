	keywords = {Humans, Adult, Angiotensin {II}, Angiotensin {II} Type 1 Receptor Blockers, Benzimidazoles, Biphenyl Compounds, Cell Proliferation, Cells, Cultured, Collagen, Cytokines, Elastin, Extracellular Matrix Proteins, Fibroblasts, Gene Expression Regulation, Intercellular Signaling Peptides and Proteins, Lung, Microscopy, Confocal, {RNA}, Messenger, Tetrazoles},
