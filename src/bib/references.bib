	journaltitle = {{JAMA} network open},
	shortjournal = {{JAMA} Netw Open},
	author = {Puskarich, Michael A. and Ingraham, Nicholas E. and Merck, Lisa H. and Driver, Brian E. and Wacker, David A. and Black, Lauren Page and Jones, Alan E. and Fletcher, Courtney V. and South, Andrew M. and Murray, Thomas A. and Lewandowski, Christopher and Farhat, Joseph and Benoit, Justin L. and Biros, Michelle H. and Cherabuddi, Kartik and Chipman, Jeffrey G. and Schacker, Timothy W. and Guirgis, Faheem W. and Voelker, Helen T. and Koopmeiners, Joseph S. and Tignanelli, Christopher J. and {Angiotensin Receptor Blocker Based Lung Protective Strategies for Inpatients With COVID-19 (ALPS-IP) Investigators}},
