	keywords = {Fibroblasts, Gene Expression Regulation, Animals, Pulmonary Fibrosis, computational modeling, fibroblasts, Hypertension, Pulmonary, Male, Mechanotransduction, Cellular, pulmonary arterial hypertension, Pulmonary Artery, Pulmonary Circulation, Rats, Rats, Sprague-Dawley, stiffness, Stress, Mechanical, stretch, Vascular Stiffness},
