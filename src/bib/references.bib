	shortjournal = {Environ Health Perspect},
	author = {Alhasan, Dana M. and Larson, Gary and Lohman, Matthew C. and Cai, Bo and {LaPorte}, Frankie B. and Miller, Maggi C. and Jackson, W. Braxton and {MacNell}, Nathaniel S. and Hirsch, Jana A. and Jackson, Chandra L.},
	date = {2024-02},
