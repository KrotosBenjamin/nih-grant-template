@article{trujillo_brain_2018,
	title = {Brain organoids and the study of neurodevelopment.},
	volume = {24},
	issn = {14714914},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1471491418301862},
	doi = {10.1016/j.molmed.2018.09.005},
	abstract = {Brain organoids are 3D self-assembled structures composed of hundreds of thousands to millions of cells that resemble the cellular organization and transcriptional and epigenetic signature of a developing human brain. Advancements using brain organoids have been made to elucidate the genetic basis of certain neurodevelopmental disorders, such as microcephaly and autism; and to investigate the impact of environmental factors to the brain, such as during Zika virus infection. It remains to be explored how far brain organoids can functionally mature and process external information. An improved brain organoid model might reproduce important aspects of the human brain in a more reproducible and high-throughput fashion. This novel and complementary approach in the neuroscience toolbox opens perspectives to understand the fundamental features of the human neurodevelopment, with implications to personalize therapeutic opportunities for neurological disorders. Copyright {\textbackslash}copyright 2018 Elsevier Ltd. All rights reserved.},
