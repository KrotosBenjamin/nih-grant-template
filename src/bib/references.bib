	keywords = {Humans, Sequence Analysis, {RNA}, Single-Cell Analysis, Bayes Theorem, Endothelial Cells, Gene Expression, Head and Neck Neoplasms, Melanoma, Melanoma, Cutaneous Malignant, Skin Neoplasms},
