	date = {2018-11-19},
	pmid = {30452450},
	pmcid = {PMC6277118},
	file = {Full Text:/home/kynon/Zotero/storage/3B9F5LN2/Delahaye et al. - 2018 - Genetic variants influence on the placenta regulat.pdf:application/pdf},
}

@article{jue_determination_2013,
