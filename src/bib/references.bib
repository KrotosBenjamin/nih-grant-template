	pmid = {12651945},
	pmcid = {PMC153088},
	file = {Full Text:/home/kynon/Zotero/storage/VEF8UBS3/Lindgren et al. - 2003 - Distinct roles of dopamine D2L and D2S receptor is.pdf:application/pdf},
}

