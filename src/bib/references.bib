	pmcid = {PMC6906116},
	file = {Full Text:/home/kynon/Zotero/storage/LDSQPUMH/Velasco et al. - 2019 - Individual brain organoids reproducibly form cell .pdf:application/pdf},
}

@article{collado-torres_regional_2019,
