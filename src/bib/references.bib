	pmid = {30685436},
	pmcid = {PMC7374759},
	file = {Full Text:/home/kynon/Zotero/storage/KVCA78HV/Lee et al. - 2019 - A Computational Framework for Genome-wide Characte.pdf:application/pdf},
}

