	keywords = {Cytokines, {RNA}, Messenger, Animals, Renin-Angiotensin System, Male, Rats, Rats, Sprague-Dawley, Angiotensin-Converting Enzyme Inhibitors, Blotting, Western, Bronchoalveolar Lavage Fluid, Captopril, Lung Diseases, {NF}-kappa B, Respiration, Artificial, Reverse Transcriptase Polymerase Chain Reaction, Ventilators, Mechanical},
