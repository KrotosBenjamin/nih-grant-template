	pmid = {33947463},
	pmcid = {PMC8094491},
	file = {Full Text:/home/kynon/Zotero/storage/DHUPDTM7/Gockley et al. - 2021 - Multi-tissue neocortical transcriptome-wide associ.pdf:application/pdf},
