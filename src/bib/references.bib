	number = {5},
	journaltitle = {Lung},
	shortjournal = {Lung},
	author = {Couluris, Marisa and Kinder, Brent W. and Xu, Ping and Gross-King, Margaret and Krischer, Jeffrey and Panos, Ralph J.},
