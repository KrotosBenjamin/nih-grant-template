	pmcid = {PMC5470330},
	keywords = {Alzheimer's disease, Cells, Cultured, Coculture Techniques, cortical neurons, Cytokines, Down-Regulation, fms-Like Tyrosine Kinase 3, human, Humans, induced pluripotent stem cell, {iPSC}, macrophage, Macrophages, microglia, Microglia, Models, Biological, neurodegeneration, Neurodegenerative Diseases, neuroinflammation, Neurons, Parkinson's disease, Phagocytosis, Pluripotent Stem Cells, Proto-Oncogene Proteins c-myb, Transcriptome},
