	url = {http://linkinghub.elsevier.com/retrieve/pii/S2452310017301944},
	doi = {10.1016/j.coisb.2017.12.007},
	abstract = {Abstract The recent advent of highly parallelizable single-cell {RNA}-sequencing technologies has opened a new window into the study of cell differentiation, commitment, and diversity. Rapid advances in the development of these technologies are being accompanied by the design of computational methods tailored to address the challenges presented by the analysis of single-cell {RNA}-sequencing data. This review provides a concise overview of some of the steps, algorithms, and approaches that are currently used in the analysis of single-cell {RNA}-sequencing data, with an emphasis on recent developments.},
	pages = {47--53},
	journaltitle = {Current Opinion in Systems Biology},
	author = {Camara, Pablo G.},
