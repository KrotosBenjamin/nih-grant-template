	date = {2022-08-23},
	pmid = {35969790},
	pmcid = {PMC9407663},
	keywords = {Humans, Genome-Wide Association Study, Polymorphism, Single Nucleotide, Brain, Brain Chemistry, convolutional neural network ({CNN}), {CpG} Islands, Deep Learning, {DNA} Methylation, {DNA} methylation quantitative trait loci ({mQTL}), {GWAS}, Neural Networks, Computer, Quantitative Trait Loci, regulatory variants, Schizophrenia, transformer},
