	keywords = {Angiotensin {II}, Animals, Mice, Pulmonary Fibrosis, Disease Progression, Hypertension, Mice, Transgenic, Receptor, Angiotensin, Type 2, Renin-Angiotensin System},
	file = {Full Text:/home/kynon/Zotero/storage/QT9288N8/Wang et al. - 2015 - Chronic Activation of the Renin-Angiotensin System.pdf:application/pdf},
