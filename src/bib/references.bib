@article{lopes_genetic_2022,
	title = {Genetic analysis of the human microglial transcriptome across brain regions, aging and disease pathologies.},
	volume = {54},
	issn = {1061-4036},
	url = {https://www.nature.com/articles/s41588-021-00976-y},
	doi = {10.1038/s41588-021-00976-y},
	abstract = {Microglia have emerged as important players in brain aging and pathology. To understand how genetic risk for neurological and psychiatric disorders is related to microglial function, large transcriptome studies are essential. Here we describe the transcriptome analysis of 255 primary human microglial samples isolated at autopsy from multiple brain regions of 100 individuals. We performed systematic analyses to investigate various aspects of microglial heterogeneities, including brain region and aging. We mapped expression and splicing quantitative trait loci and showed that many neurological disease susceptibility loci are mediated through gene expression or splicing in microglia. Fine-mapping of these loci nominated candidate causal variants that are within microglia-specific enhancers, finding associations with microglial expression of {USP}6NL for Alzheimer's disease and P2RY12 for Parkinson's disease. We have built the most comprehensive catalog to date of genetic effects on the microglial transcriptome and propose candidate functional variants in neurological and psychiatric disorders. {\textbackslash}copyright 2022. The Author(s), under exclusive licence to Springer Nature America, Inc.},
	pages = {4--17},
	number = {1},
	journaltitle = {Nature Genetics},
	author = {Lopes, Katia de Paiva and Snijders, Gijsje J L and Humphrey, Jack and Allan, Amanda and Sneeboer, Marjolein A M and Navarro, Elisa and Schilder, Brian M and Vialle, Ricardo A and Parks, Madison and Missall, Roy and van Zuiden, Welmoed and Gigase, Frederieke A J and Kübler, Raphael and van Berlekom, Amber Berdenis and Hicks, Emily M and Bӧttcher, Chotima and Priller, Josef and Kahn, René S and de Witte, Lot D and Raj, Towfique},
	urldate = {2023-08-23},
	date = {2022-01-06},
