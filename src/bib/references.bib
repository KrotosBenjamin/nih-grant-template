	author = {Upton, Kyle R and Gerhardt, Daniel J and Jesuadian, J Samuel and Richardson, Sandra R and Sánchez-Luque, Francisco J and Bodea, Gabriela O and Ewing, Adam D and Salvador-Palomeque, Carmen and van der Knaap, Marjo S and Brennan, Paul M and Vanderver, Adeline and Faulkner, Geoffrey J},
	urldate = {2018-02-14},
	date = {2015-04-09},
	pmid = {25860606},
	pmcid = {PMC4398972},
	file = {Full Text:/home/kynon/Zotero/storage/E945Q7HG/Upton et al. - 2015 - Ubiquitous L1 mosaicism in hippocampal neurons..pdf:application/pdf},
}

