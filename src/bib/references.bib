	keywords = {Female, Humans, Disease Progression, Male, Losartan, Aged, Middle Aged, Respiratory Function Tests, Aged, 80 and over, Dyspnea, Exercise Test, Idiopathic Pulmonary Fibrosis, Pilot Projects, Treatment Outcome},
