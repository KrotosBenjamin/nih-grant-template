	urldate = {2015-02-12},
	date = {2014-01-22},
	pmid = {24389010},
	file = {Full Text:/home/kynon/Zotero/storage/DTHG99AD/Bundo et al. - 2014 - Increased l1 retrotransposition in the neuronal ge.pdf:application/pdf},
