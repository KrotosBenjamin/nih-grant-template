	author = {Genovese, Giulio and Fromer, Menachem and Stahl, Eli A and Ruderfer, Douglas M and Chambert, Kimberly and Landén, Mikael and Moran, Jennifer L and Purcell, Shaun M and Sklar, Pamela and Sullivan, Patrick F and Hultman, Christina M and {McCarroll}, Steven A},
	urldate = {2023-03-14},
