	pmid = {31903107},
	pmcid = {PMC6929610},
	file = {Full Text:/home/kynon/Zotero/storage/449WPN98/Zhang et al. - 2020 - Microglia exacerbate white matter injury via compl.pdf:application/pdf},
}

