	pmcid = {PMC6761727},
	file = {Full Text:/home/kynon/Zotero/storage/GIWLN7A7/Price et al. - 2019 - Divergent neuronal DNA methylation patterns across.pdf:application/pdf},
}

@article{kisiswa_t-type_2017,
	title = {T-type Ca2+ channels are required for enhanced sympathetic axon growth by {TNF}{\textbackslash}alpha reverse signalling.},
	volume = {7},
	url = {http://dx.doi.org/10.1098/rsob.160288},
	doi = {10.1098/rsob.160288},
	abstract = {Tumour necrosis factor receptor 1 ({TNFR}1)-activated {TNF}{\textbackslash}alpha reverse signalling, in which membrane-integrated {TNF}{\textbackslash}alpha functions as a receptor for {TNFR}1, enhances axon growth from developing sympathetic neurons and plays a crucial role in establishing sympathetic innervation. Here, we have investigated the link between {TNF}{\textbackslash}alpha reverse signalling and axon growth in cultured sympathetic neurons. {TNFR}1-activated {TNF}{\textbackslash}alpha reverse signalling promotes Ca2+ influx, and highly selective T-type Ca2+ channel inhibitors, but not pharmacological inhibitors of L-type, N-type and P/Q-type Ca2+ channels, prevented enhanced axon growth. T-type Ca2+ channel-specific inhibitors eliminated Ca2+ spikes promoted by {TNF}{\textbackslash}alpha reverse signalling in axons and prevented enhanced axon growth when applied locally to axons, but not when applied to cell somata. Blocking action potential generation did not affect the effect of {TNF}{\textbackslash}alpha reverse signalling on axon growth, suggesting that propagated action potentials are not required for enhanced axon growth. {TNF}{\textbackslash}alpha reverse signalling enhanced protein kinase C ({PKC}) activation, and pharmacological inhibition of {PKC} prevented the axon growth response. These results suggest that {TNF}{\textbackslash}alpha reverse signalling promotes opening of T-type Ca2+ channels along sympathetic axons, which is required for enhanced axon growth. {\textbackslash}copyright 2017 The Authors.},
	number = {1},
	journaltitle = {Open biology},
	author = {Kisiswa, Lilian and Erice, Clara and Ferron, Laurent and Wyatt, Sean and Osório, Catarina and Dolphin, Annette C and Davies, Alun M},
	urldate = {2020-04-13},
