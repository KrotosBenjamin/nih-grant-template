	pmid = {29610479},
	pmcid = {PMC5905669},
	file = {Accepted Version:/home/kynon/Zotero/storage/5H9QJ7ZN/van der Wijst et al. - 2018 - Single-cell RNA sequencing identifies celltype-spe.pdf:application/pdf},
}

@article{gusev_transcriptome-wide_2018,
	title = {Transcriptome-wide association study of schizophrenia and chromatin activity yields mechanistic disease insights.},
