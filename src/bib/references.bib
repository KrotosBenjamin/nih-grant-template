@article{a_gadd_genetic_2021,
	title = {The genetic and epigenetic profile of serum S100{\textbackslash}beta in the Lothian Birth Cohort 1936 and its relationship to Alzheimer's disease.},
	volume = {6},
	url = {http://dx.doi.org/10.12688/wellcomeopenres.17322.2},
	doi = {10.12688/wellcomeopenres.17322.2},
	abstract = {Background: Circulating S100 calcium-binding protein (S100{\textbackslash}beta) is a marker of brain inflammation that has been associated with a range of neurological conditions. To provide insight into the molecular regulation of S100{\textbackslash}beta and its potential causal associations with Alzheimer's disease, we carried out genome- and epigenome-wide association studies ({GWAS}/{EWAS}) of serum S100{\textbackslash}beta levels in older adults and performed Mendelian randomisation with Alzheimer's disease. Methods: {GWAS} (N=769, mean age 72.5 years, sd = 0.7) and {EWAS} (N=722, mean age 72.5 years, sd = 0.7) of S100{\textbackslash}beta levels were performed in participants from the Lothian Birth Cohort 1936. Conditional and joint analysis ({COJO}) was used to identify independent loci. Expression quantitative trait locus ({eQTL}) analyses were performed for lead loci that had genome-wide significant associations with S100{\textbackslash}beta. Bidirectional, two-sample Mendelian randomisation was used to test for causal associations between S100{\textbackslash}beta and Alzheimer's disease. Colocalisation between S100{\textbackslash}beta and Alzheimer's disease {GWAS} loci was also examined. Results: We identified 154 {SNPs} from chromosome 21 that associated (P{\textbackslash}textless 5x10 -8) with S100{\textbackslash}beta protein levels. The lead variant was located in the S100{\textbackslash}beta gene (rs8128872, P=5.0x10 -17). We found evidence that two independent causal variants existed for both transcription of S100{\textbackslash}beta and S100{\textbackslash}beta protein levels in our {eQTL} analyses . No {CpG} sites were associated with S100{\textbackslash}beta levels at the epigenome-wide significant level (P{\textbackslash}textless 3.6x10 -8); the lead probe was cg06833709 (P=5.8x10 -6), which mapped to the {LGI}1 gene. There was no evidence of a causal association between S100{\textbackslash}beta levels and Alzheimer's disease or vice versa and no evidence for colocalisation between S100{\textbackslash}beta and Alzheimer's disease loci. Conclusions: These data provide insight into the molecular regulators of S100{\textbackslash}beta levels. This context may aid in understanding the role of S100{\textbackslash}beta in brain inflammation and neurological disease. Copyright: {\textbackslash}copyright 2022 A Gadd D et al.},
	pages = {306},
	journaltitle = {Wellcome Open Research},
	author = {A Gadd, Danni and I {McGeachan}, Robert and F Hillary, Robert and L {McCartney}, Daniel and E Harris, Sarah and A Sherwood, Roy and Abbott, N Joan and R Cox, Simon and E Marioni, Riccardo},
	urldate = {2023-03-23},
	date = {2021},
	pmid = {35028426},
	pmcid = {PMC8686327},
	file = {Full Text:/home/kynon/Zotero/storage/TAD5MWST/A Gadd et al. - 2021 - The genetic and epigenetic profile of serum S100b.pdf:application/pdf},
}

@article{cotto_integrated_2023,
	title = {Integrated analysis of genomic and transcriptomic data for the discovery of splice-associated variants in cancer.},
	volume = {14},
	url = {http://dx.doi.org/10.1038/s41467-023-37266-6},
	doi = {10.1038/s41467-023-37266-6},
	abstract = {Somatic mutations within non-coding regions and even exons may have unidentified regulatory consequences that are often overlooked in analysis workflows. Here we present {RegTools} ( www.regtools.org ), a computationally efficient, free, and open-source software package designed to integrate somatic variants from genomic data with splice junctions from bulk or single cell transcriptomic data to identify variants that may cause aberrant splicing. We apply {RegTools} to over 9000 tumor samples with both tumor {DNA} and {RNA} sequence data. {RegTools} discovers 235,778 events where a splice-associated variant significantly increases the splicing of a particular junction, across 158,200 unique variants and 131,212 unique junctions. To characterize these somatic variants and their associated splice isoforms, we annotate them with the Variant Effect Predictor, {SpliceAI}, and Genotype-Tissue Expression junction counts and compare our results to other tools that integrate genomic and transcriptomic data. While many events are corroborated by the aforementioned tools, the flexibility of {RegTools} also allows us to identify splice-associated variants in known cancer drivers, such as {TP}53, {CDKN}2A, and B2M, and other genes. {\textbackslash}copyright 2023. The Author(s).},
