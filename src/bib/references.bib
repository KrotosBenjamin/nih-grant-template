	keywords = {Humans, Chromosome Mapping, Gene Expression Profiling, Genetic Predisposition to Disease, Genome-Wide Association Study, Phenotype, Polymorphism, Single Nucleotide},
	file = {Full Text:/home/kynon/Zotero/storage/879LI9DD/Gamazon et al. - 2015 - A gene-based association method for mapping traits.pdf:application/pdf},
}

@article{manolio_finding_2009,
