@article{yuan_fine-mapping_2024,
	title = {Fine-mapping across diverse ancestries drives the discovery of putative causal variants underlying human complex traits and diseases},
	volume = {56},
	issn = {1546-1718},
	doi = {10.1038/s41588-024-01870-z},
	abstract = {Genome-wide association studies ({GWAS}) of human complex traits or diseases often implicate genetic loci that span hundreds or thousands of genetic variants, many of which have similar statistical significance. While statistical fine-mapping in individuals of European ancestry has made important discoveries, cross-population fine-mapping has the potential to improve power and resolution by capitalizing on the genomic diversity across ancestries. Here we present {SuSiEx}, an accurate and computationally efficient method for cross-population fine-mapping. {SuSiEx} integrates data from an arbitrary number of ancestries, explicitly models population-specific allele frequencies and linkage disequilibrium patterns, accounts for multiple causal variants in a genomic region and can be applied to {GWAS} summary statistics. We comprehensively assessed the performance of {SuSiEx} using simulations. We further showed that {SuSiEx} improves the fine-mapping of a range of quantitative traits available in both the {UK} Biobank and Taiwan Biobank, and improves the fine-mapping of schizophrenia-associated loci by integrating {GWAS} across East Asian and European ancestries.},
	pages = {1841--1850},
	number = {9},
	journaltitle = {Nature Genetics},
	shortjournal = {Nat Genet},
	author = {Yuan, Kai and Longchamps, Ryan J. and Pardiñas, Antonio F. and Yu, Mingrui and Chen, Tzu-Ting and Lin, Shu-Chin and Chen, Yu and Lam, Max and Liu, Ruize and Xia, Yan and Guo, Zhenglin and Shi, Wenzhao and Shen, Chengguo and {Schizophrenia Workgroup of Psychiatric Genomics Consortium} and Daly, Mark J. and Neale, Benjamin M. and Feng, Yen-Chen A. and Lin, Yen-Feng and Chen, Chia-Yen and O'Donovan, Michael C. and Ge, Tian and Huang, Hailiang},
	date = {2024-09},
	pmid = {39187616},
	keywords = {Humans, Chromosome Mapping, Genetic Predisposition to Disease, Genome-Wide Association Study, Polymorphism, Single Nucleotide, Quantitative Trait Loci, Schizophrenia, Computer Simulation, Genetic Variation, White People, Genome, Human, East Asian People, Gene Frequency, Linkage Disequilibrium, Models, Genetic, Multifactorial Inheritance},
}

@article{parhizkar_apoe_2022,
	title = {{APOE} mediated neuroinflammation and neurodegeneration in Alzheimer's disease},
	volume = {59},
	issn = {1096-3618},
	doi = {10.1016/j.smim.2022.101594},
	abstract = {Neuroinflammation is a central mechanism involved in neurodegeneration as observed in Alzheimer's disease ({AD}), the most prevalent form of neurodegenerative disease. Apolipoprotein E4 ({APOE}4), the strongest genetic risk factor for {AD}, directly influences disease onset and progression by interacting with the major pathological hallmarks of {AD} including amyloid-β plaques, neurofibrillary tau tangles, as well as neuroinflammation. Microglia and astrocytes, the two major immune cells in the brain, exist in an immune-vigilant state providing immunological defense as well as housekeeping functions that promote neuronal well-being. It is becoming increasingly evident that under disease conditions, these immune cells become progressively dysfunctional in regulating metabolic and immunoregulatory pathways, thereby promoting chronic inflammation-induced neurodegeneration. Here, we review and discuss how {APOE} and specifically {APOE}4 directly influences amyloid-β and tau pathology, and disrupts microglial as well as astroglial immunomodulating functions leading to chronic inflammation that contributes to neurodegeneration in {AD}.},
	pages = {101594},
	journaltitle = {Seminars in Immunology},
	shortjournal = {Semin Immunol},
	author = {Parhizkar, Samira and Holtzman, David M.},
	date = {2022-01},
	pmid = {35232622},
	pmcid = {PMC9411266},
	keywords = {Humans, Alzheimer Disease, Inflammation, Microglia, Astrocytes, Alzheimer’s disease, Apolipoprotein E, Apolipoprotein E4, Neurodegeneration, Neurodegenerative Diseases, Neuroinflammatory Diseases},
	file = {Full Text PDF:/home/kynon/Zotero/storage/GITMZQ33/Parhizkar and Holtzman - 2022 - APOE mediated neuroinflammation and neurodegeneration in Alzheimer's disease.pdf:application/pdf},
}

@article{naslavsky_global_2022,
	title = {Global and local ancestry modulate {APOE} association with Alzheimer’s neuropathology and cognitive outcomes in an admixed sample},
	volume = {27},
	rights = {2022 The Author(s)},
	issn = {1476-5578},
	url = {https://www.nature.com/articles/s41380-022-01729-x},
	doi = {10.1038/s41380-022-01729-x},
	abstract = {Dementia is more prevalent in Blacks than in Whites, likely due to a combination of environmental and biological factors. Paradoxically, clinical studies suggest an attenuation of {APOE} ε4 risk of dementia in African ancestry ({AFR}), but a dearth of neuropathological data preclude the interpretation of the biological factors underlying these findings, including the association between {APOE} ε4 risk and Alzheimer’s disease ({AD}) pathology, the most frequent cause of dementia. We investigated the interaction between African ancestry, {AD}-related neuropathology, {APOE} genotype, and functional cognition in a postmortem sample of 400 individuals with a range of {AD} pathology severity and lack of comorbid neuropathology from a cohort of community-dwelling, admixed Brazilians. Increasing proportions of African ancestry ({AFR}) correlated with a lower burden of neuritic plaques ({NP}). However, for individuals with a severe burden of {NP} and neurofibrillary tangles ({NFT}), {AFR} proportion was associated with worse Clinical Dementia Rating sum of boxes ({CDR}-{SOB}). Among {APOE} ε4 carriers, the association between {AFR} proportion and {CDR}-{SOB} disappeared. {APOE} local ancestry inference of a subset of 309 individuals revealed that, in {APOE} ε4 noncarriers, non-European {APOE} background correlated with lower {NP} burden and, also, worse cognitive outcomes than European {APOE} when adjusting by {NP} burden. Finally, {APOE} ε4 was associated with worse {AD} neuropathological burden only in a European {APOE} background. {APOE} genotype and its association with {AD} neuropathology and clinical pattern are highly influenced by ancestry, with {AFR} associated with lower {NP} burden and attenuated {APOE} ε4 risk compared to European ancestry.},
	pages = {4800--4808},
	number = {11},
	journaltitle = {Molecular Psychiatry},
	shortjournal = {Mol Psychiatry},
	author = {Naslavsky, Michel Satya and Suemoto, Claudia K. and Brito, Luciano Abreu and Scliar, Marília Oliveira and Ferretti-Rebustini, Renata Eloah and Rodriguez, Roberta Diehl and Leite, Renata E. P. and Araujo, Nathalia Matta and Borda, Victor and Tarazona-Santos, Eduardo and Jacob-Filho, Wilson and Pasqualucci, Carlos and Nitrini, Ricardo and Yaffe, Kristine and Zatz, Mayana and Grinberg, Lea T.},
	urldate = {2024-11-01},
	date = {2022-11},
	langid = {english},
	note = {Publisher: Nature Publishing Group},
	keywords = {Genetics, Neuroscience, Predictive markers},
	file = {Full Text PDF:/home/kynon/Zotero/storage/S9FGLTDQ/Naslavsky et al. - 2022 - Global and local ancestry modulate APOE association with Alzheimer’s neuropathology and cognitive ou.pdf:application/pdf},
}

@article{lake_multi-ancestry_2023,
	title = {Multi-ancestry meta-analysis and fine-mapping in Alzheimer’s disease},
	volume = {28},
	issn = {1359-4184, 1476-5578},
	url = {https://www.nature.com/articles/s41380-023-02089-w},
	doi = {10.1038/s41380-023-02089-w},
	abstract = {Abstract
            Genome-wide association studies ({GWAS}) of Alzheimer’s disease are predominantly carried out in European ancestry individuals despite the known variation in genetic architecture and disease prevalence across global populations. We leveraged published {GWAS} summary statistics from European, East Asian, and African American populations, and an additional {GWAS} from a Caribbean Hispanic population using previously reported genotype data to perform the largest multi-ancestry {GWAS} meta-analysis of Alzheimer’s disease and related dementias to date. This method allowed us to identify two independent novel disease-associated loci on chromosome 3. We also leveraged diverse haplotype structures to fine-map nine loci with a posterior probability {\textgreater}0.8 and globally assessed the heterogeneity of known risk factors across populations. Additionally, we compared the generalizability of multi-ancestry- and single-ancestry-derived polygenic risk scores in a three-way admixed Colombian population. Our findings highlight the importance of multi-ancestry representation in uncovering and understanding putative factors that contribute to risk of Alzheimer’s disease and related dementias.},
	pages = {3121--3132},
	number = {7},
	journaltitle = {Molecular Psychiatry},
	shortjournal = {Mol Psychiatry},
	author = {Lake, Julie and Warly Solsberg, Caroline and Kim, Jonggeol Jeffrey and Acosta-Uribe, Juliana and Makarious, Mary B. and Li, Zizheng and Levine, Kristin and Heutink, Peter and Alvarado, Chelsea X. and Vitale, Dan and Kang, Sarang and Gim, Jungsoo and Lee, Kun Ho and Pina-Escudero, Stefanie D. and Ferrucci, Luigi and Singleton, Andrew B. and Blauwendraat, Cornelis and Nalls, Mike A. and Yokoyama, Jennifer S. and Leonard, Hampton L.},
	urldate = {2024-11-01},
