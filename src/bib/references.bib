	urldate = {2021-07-31},
	date = {2016-05-15},
	pmid = {26708335},
	pmcid = {PMC4866519},
	file = {Full Text:/home/kynon/Zotero/storage/52FHYU8M/Ongen et al. - 2016 - Fast and efficient QTL mapper for thousands of mol.pdf:application/pdf},
}

