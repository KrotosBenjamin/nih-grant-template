	urldate = {2024-07-17},
	date = {2024},
	note = {Conference Name: {IEEE} Transactions on Neural Networks and Learning Systems},
	keywords = {Gene expression, Accuracy, Causality inference, Cause effect analysis, gene regulatory networks ({GRNs}), Graph neural networks, graph neural networks ({GNNs}), Iron, Regulation, single-cell {RNA} sequencing ({RNA}-seq), Vectors},
	file = {IEEE Xplore Abstract Record:/home/kynon/Zotero/storage/FQZ6U52C/10562353.html:text/html;IEEE Xplore Full Text PDF:/home/kynon/Zotero/storage/GURZ7EML/Wang et al. - 2024 - GRACE Unveiling Gene Regulatory Networks With Cau.pdf:application/pdf},
}

@article{cui_scgpt_2024,
