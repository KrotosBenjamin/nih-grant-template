	number = {16},
	journaltitle = {Human Molecular Genetics},
	author = {Luo, Yang and Li, Xinyi and Wang, Xin and Gazal, Steven and Mercader, Josep Maria and {23} and Team, Me Research and Consortium, SIGMA Type 2 Diabetes and Neale, Benjamin M and Florez, Jose C and Auton, Adam and Price, Alkes L and Finucane, Hilary K and Raychaudhuri, Soumya},
