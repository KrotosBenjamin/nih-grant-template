	pmid = {28892061},
	pmcid = {PMC6133304},
	file = {Accepted Version:/home/kynon/Zotero/storage/HFQXKRYY/Gazal et al. - 2017 - Linkage disequilibrium-dependent architecture of h.pdf:application/pdf},
}

